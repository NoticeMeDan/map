<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menuBar" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1"
		 fx:controller="com.noticemedan.mappr.viewmodel.MenuBarController">
	<Menu mnemonicParsing="false" text="Fil">
		<MenuItem fx:id="menuCreateMapFromOsm" mnemonicParsing="false" text="Indlæs nyt kort fra OSM fil"/>
		<MenuItem mnemonicParsing="false" text="Indlæs kort"/>
		<MenuItem mnemonicParsing="false" text="Slet kort"/>
		<MenuItem mnemonicParsing="false" disable="true" text="Current map: Bornholm"/>
	</Menu>
	<Menu mnemonicParsing="false" text="Vindue">
		<MenuItem mnemonicParsing="false" text="Vis alle kort"/>
		<MenuItem mnemonicParsing="false" text="Vis favoritsteder"/>
		<MenuItem mnemonicParsing="false" text="Vis addressesøgning"/>
		<MenuItem mnemonicParsing="false" text="Vis navigering"/>

		<Menu mnemonicParsing="false" text="Farvestil">
			<MenuItem fx:id="standardColor" mnemonicParsing="false" text="Standard"/>
			<MenuItem fx:id="colorBlind" mnemonicParsing="false" text="Farveblind"/>
		</Menu>
	</Menu>
	<Menu mnemonicParsing="false" text="Udvikler">
		<MenuItem fx:id="menuShowFPS" mnemonicParsing="false" text="Vis FPS"/>
		<MenuItem mnemonicParsing="false" text="Vis KDTree linjer"/>
		<MenuItem fx:id="menuShowReversedBorders" mnemonicParsing="false" text="Vis range-search"/>
		<MenuItem fx:id="menuShowDijkstraNetwork" mnemonicParsing="false" text="Vis dijkstra-netværk"/>
		<MenuItem fx:id="menuShowShortestPath" mnemonicParsing="false" text="Vis shortest path"/>
	</Menu>
</MenuBar>
